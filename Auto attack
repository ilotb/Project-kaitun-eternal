while wait(.2) do for i,v in pairs(workspace.Debris.Monsters:GetChildren()) do if v:FindFirstChild("HumanoidRootPart") and not (v:GetAttribute("Health") == "100") and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude < 30 then for i = 1, 10 do local args = { { Id = v.Name, Action = "_Mouse_Click" } } game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("To_Server"):FireServer(unpack(args)) end end end end
