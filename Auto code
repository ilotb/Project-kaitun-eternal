-- loop through all codes
for _, code in ipairs(codes) do
    local args = {
        [1] = {
            Action = "_Redeem_Code",
            Text = code
        }
    }

    game:GetService("ReplicatedStorage")
        :WaitForChild("Events")
        :WaitForChild("To_Server")
        :FireServer(unpack(args))

    task.wait(0.2) -- small delay so it doesnâ€™t spam too fast
end
